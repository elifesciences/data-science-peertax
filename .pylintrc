[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    too-many-statements,
    too-many-branches,
    too-many-instance-attributes,
    no-else-return,
    no-self-use,
    fixme,
    duplicate-code

[TYPECHECK]
ignored-classes=
    lxml.builder.ElementMaker
extension-pkg-whitelist=
    lxml
